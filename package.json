{
  "name": "gh-actions-analyzer",
  "displayName": "GitHub Actions 분석기",
  "description": "GitHub Actions 실패 로그를 분석하고 LLM 프롬프트로 만들어줍니다.",
  "version": "0.0.1",
  "publisher": "angkmfirefoxygal",
  "engines": {
    "vscode": "^1.80.0"
  },
  "main": "./out/extension.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "eslint . --ext ts"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.analyzeGitHubActions",
        "title": "GitHub Actions 실패 로그 분석"
      },
      {
        "command": "extension.deleteGitHubToken",
        "title": "Delete GitHub Token"
      },
      {
        "command": "extension.openDashboard",
        "title": "MAD Ops: Dashboard"
      }
    ]
  },
  "dependencies": {
    "@octokit/rest": "^20.0.0",
    "dotenv": "^17.2.0",
    "jszip": "^3.10.1",
    "node-fetch": "^2.7.0",
    "openai": "^5.10.1",
    "simple-git": "^3.28.0"
  },
  "devDependencies": {
    "@types/jszip": "^3.4.0",
    "@types/node": "^20.19.9",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angkmfirefoxygal/oss.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/angkmfirefoxygal/oss/issues"
  },
  "homepage": "https://github.com/angkmfirefoxygal/oss#readme"
}
