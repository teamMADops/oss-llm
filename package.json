{
  "publisher": "madops-hj",
  "name": "gh-actions-analyzer",
  "displayName": "MADops: GitHub Actions 분석기",
  "description": "GitHub Actions 실패 로그를 자동 분석하고, 원인과 해결책을 AI가 제시합니다.",
  "version": "0.0.8",
  "icon": "images/logo-128.png",
  "galleryBanner": {
    "color": "#1c1c1c",
    "theme": "dark"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "AI"
  ],
  "extensionKind": [
    "workspace"
  ],
  "engines": {
    "vscode": "^1.101.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.setRepository",
        "title": "GitHub Actions: Set or Update Repository"
      },
      {
        "command": "extension.clearRepository",
        "title": "GitHub Actions: Remove Saved Repository"
      },
      {
        "command": "extension.showRepository",
        "title": "GitHub Actions: Show Current Repository"
      },
      {
        "command": "extension.loginGithub",
        "title": "GitHub: Sign In (VS Code Account Authentication)"
      },
      {
        "command": "extension.logoutGithub",
        "title": "GitHub: Sign Out (VS Code Account Authentication)"
      },
      {
        "command": "extension.analyzeGitHubActions",
        "title": "GitHub Actions: Analyze Failed Steps and Generate LLM Prompts"
      },
      {
        "command": "extension.openDashboard",
        "title": "MAD Ops: Open Dashboard"
      },
      {
        "command": "extension.setOpenAiKey",
        "title": "OpenAI: Set API Key"
      },
      {
        "command": "extension.clearOpenAiKey",
        "title": "OpenAI: Delete API Key"
      }
    ]
  },
  "scripts": {
    "server:start": "tsx server/index.ts",
    "server:dev": "tsx watch server/index.ts",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "setup:webview": "cd src/webview-react-app && npm install",
    "build:webview": "cd src/webview-react-app && npm run build",
    "release": "npm version patch && cross-var \"git tag -a RELEASE-v$npm_package_version -m 'Release v$npm_package_version' && git push origin RELEASE-v$npm_package_version\""
  },
  "dependencies": {
    "@octokit/rest": "^19.0.13",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "eventsource": "^4.0.0",
    "express": "^5.1.0",
    "jszip": "^3.10.1",
    "node-fetch": "^2.7.0",
    "openai": "^5.12.2",
    "simple-git": "^3.28.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/eventsource": "^1.1.15",
    "@types/express": "^5.0.3",
    "@types/jszip": "^3.4.0",
    "@types/node": "^20.19.11",
    "@types/node-fetch": "^2.6.13",
    "@types/vscode": "^1.101.0",
    "@vscode/test-electron": "^2.4.2",
    "cross-var": "^1.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teamMADops/oss-llm.git"
  },
  "keywords": [
    "github-actions",
    "ai",
    "llm",
    "debugging",
    "devops",
    "automation"
  ],
  "author": "MADops Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teamMADops/oss-llm/issues"
  },
  "homepage": "https://github.com/teamMADops/oss-llm#readme"
}
