{
  "name": "gh-actions-analyzer",
  "displayName": "GitHub Actions 분석기",
  "description": "GitHub Actions 실패 로그를 분석하고 LLM 프롬프트로 만들어줍니다.",
  "version": "0.0.1",
  "publisher": "angkmfirefoxygal",
  "engines": {
    "vscode": "^1.101.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
          
      { "command": "extension.setRepository", 
        "title": "GitHub Actions: 레포 저장/수정"
      },
      { "command": "extension.clearRepository", 
        "title": "GitHub Actions: 저장된 레포 삭제" 
      },
      { "command": "extension.showRepository", 
        "title": "GitHub Actions: 현재 레포 보기" 
      },
      {
        "command": "extension.analyzeGitHubActions",
        "title": "GitHub Actions: 실패 Step 분석 및 LLM 프롬프트 생성"
      },
      {
        "command": "extension.helloWorld",
        "title": "Dev: Hello World Webview 열기"
      },
      {
        "command": "extension.openWorkflowEditor",
        "title": "GitHub Actions: Workflow 에디터 열기"
      },
      {
        "command": "extension.deleteGitHubToken",
        "title": "GitHub Actions: 저장된 GitHub 토큰 삭제"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "dotenv": "^17.2.0",
    "jszip": "^3.10.1",
    "node-fetch": "^3.3.2",
    "openai": "^5.10.1",
    "simple-git": "^3.28.0"
  },
  "devDependencies": {
    "@types/jszip": "^3.4.0",
    "@types/node": "^20.19.9",
    "@types/vscode": "^1.101.0",
    "@vscode/test-electron": "^2.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angkmfirefoxygal/oss.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/angkmfirefoxygal/oss/issues"
  },
  "homepage": "https://github.com/angkmfirefoxygal/oss#readme"
}
